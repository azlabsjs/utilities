{
  "version": "0.1.12",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "ts-build build",
    "lint": "ts-build lint",
    "prepare": "git config core.hookspath .githooks && ts-build build",
    "size": "size-limit",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "name": "@azlabsjs/utilities",
  "author": "azandrew-sidoine",
  "module": "dist/esm/index.mjs",
  "size-limit": [
    {
      "path": "dist/cjs/index.cjs",
      "limit": "10 KB"
    },
    {
      "path": "dist/esm/index.mjs",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@azlabsjs/ts-build": "^0.4.21",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.3.5",
    "size-limit": "^7.0.8",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@azlabsjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azlabsjs/utilities.git"
  }
}
