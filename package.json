{
  "version": "0.1.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "ts-build build",
    "lint": "ts-build lint",
    "prepare": "git config core.hookspath .githooks && ts-build build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "test": "jest",
    "betterer": "betterer --strict"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "name": "@iazlabs/utilities",
  "author": "azandrew-sidoine",
  "module": "dist/esm/index.js",
  "size-limit": [
    {
      "path": "dist/cjs/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/esm/index.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@betterer/cli": "^5.1.6",
    "@betterer/typescript": "^5.1.6",
    "@iazlabs/ts-build": "^0.1.6",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.3.5",
    "size-limit": "^7.0.8",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@iazlabs"
  }
}
