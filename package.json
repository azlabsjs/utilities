{
  "version": "0.2.2",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/esm/index.d.ts",
  "typings": "dist/esm/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "ts-build build",
    "lint": "ts-build lint",
    "prepare": "git config core.hookspath .githooks && ts-build build",
    "size": "size-limit",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@azlabsjs/utilities",
  "author": "azandrew-sidoine",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "typings": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "typings": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "size-limit": [
    {
      "path": "dist/cjs/index.cjs",
      "limit": "10 KB"
    },
    {
      "path": "dist/esm/index.mjs",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@azlabsjs/ts-build": "^0.5.15",
    "@babel/runtime": "^7.27.6",
    "@eslint/js": "^9.30.0",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.30.0",
    "husky": "^9.1.7",
    "jest": "^30.0.3",
    "jest-watch-typeahead": "^3.0.1",
    "prettier": "^3.6.2",
    "size-limit": "^11.2.0",
    "structured-clone-polyfill": "^0.0.2",
    "ts-jest": "^29.4.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@azlabsjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azlabsjs/utilities.git"
  }
}
